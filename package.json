    "alphavantage": "^1.0.0",
    "bootstrap": "^4.0.0-alpha.6"
  },
  "devDependencies": {
    "lite-server": "^2.3.0",
    "node-sass": "^4.7.2",
    "onchange": "^3.3.0",
    "parallelshell": "^3.0.2"
  }
